{
  "family": "cpfu_nuxt_docker",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "cpfu_nuxt_docker",
      "image": "cpfu_nuxt_docker",
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 443,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "executionRoleArn": "ecsTaskExecutionRole",
  "cpu": "256",
  "memory": "512"
}